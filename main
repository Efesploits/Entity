--go use entity hub--efe is COOOOL--
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("Entity Hub", "DarkTheme")
--sekmeler--
local Tab1 = Window:NewTab("Credits")
local Tab2 = Window:NewTab("Reanimation")
local Tab3 = Window:NewTab("No Hat Scripts")
local Tab4 = Window:NewTab("Hat requiring Scripts")
local tab5 = Window:NewTab("Setttings")

--yazılar credits için--
local Section = Tab1:NewSection("Credits")

Section:NewLabel("Gui,Script Converting,Reanims : Efe skid#1981")


--kategori reanim için--
